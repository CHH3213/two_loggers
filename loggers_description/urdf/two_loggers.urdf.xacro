<?xml version="1.0" ?>
<!-- This model started from link_log -->

<robot name="two_loggers" xmlns:xacro="https://www.ros.org/wiki/xacro" >
    
  <!-- Constants for robot dimensions -->
  <xacro:property name="PI" value="3.1415926535897931"/>
  <xacro:property name="EPSILON" value="1e-6"/> <!-- a small number for default inertia -->

  <!-- Import all Gazebo-customization elements, including Gazebo colors -->
  <xacro:include filename="$(find loggers_description)/urdf/two_loggers.gazebo" />
  <!-- Import Rviz colors -->
  <xacro:include filename="$(find loggers_description)/urdf/materials.xacro" />

  <!-- Use a dummy link, or Gazebo would not take care of root link's inertia -->
  <link name="world"/>

  <joint name="joint_world_log" type="floating">
    <parent link="world"/>
    <child link="link_log"/>
  </joint>

  <link name="link_log">
    <visual>
      <origin rpy="0 ${PI/2} 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="2" radius="0.01"/>
      </geometry>
      <material name="red"/>
    </visual>
    <collision>
      <origin rpy="0 ${PI/2} 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="2" radius="0.01"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <origin rpy="0 ${PI/2} 0" xyz="0 0 0"/>
      <inertia ixx="${EPSILON}" ixy="0" ixz="0" iyy="${EPSILON}" iyz="0" izz="${EPSILON}"/>
    </inertial>
  </link>

  <joint name="joint_log_hat0" type="revolute">
    <origin rpy="0 0 0" xyz="-1 0 0"/>
    <parent link="link_log"/>
    <child link="link_hat_0"/>
    <axis rpy="0 0 0" xyz="0 0 1"/>
    <limit effort="0" lower="0" upper="0" velocity="0"/>
  </joint>

  <link name="link_hat_0">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 -0.04"/>
      <geometry>
        <cylinder length="0.08" radius="0.02"/>
      </geometry>
      <material name="orange"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 -0.04"/>
      <geometry>
        <cylinder length="0.08" radius="0.02"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <origin rpy="0 0 0" xyz="0 0 -0.04"/>
      <inertia ixx="${EPSILON}" ixy="0" ixz="0" iyy="${EPSILON}" iyz="0" izz="${EPSILON}"/>
    </inertial>
  </link>

  <joint name="joint_hat0_chassis0" type="continuous">
    <origin rpy="0 0 0" xyz="0.0 0 -0.13"/>
    <parent link="link_hat_0"/>
    <child link="link_chassis_0" />
    <axis rpy="0 0 0" xyz="0 0 1"/>
  </joint>

  <link name="link_chassis_0">
    <visual>
      <origin rpy="0 0 ${-PI/2}" xyz="0 0 0"/>
      <geometry>
        <box size="0.4 0.2 0.1"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin rpy="0 0 ${-PI/2}" xyz="0 0 0"/>
      <geometry>
        <box size="0.4 0.2 0.1"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="2"/>
      <origin rpy="0 0 ${-PI/2}" xyz="0 0 0"/>
      <inertia ixx="${EPSILON}" ixy="0" ixz="0" iyy="${EPSILON}" iyz="0" izz="${EPSILON}"/>
    </inertial>
  </link>

  <joint name="joint_chassis0_caster0" type="fixed">
    <origin rpy="0 0 0" xyz="0 -0.15 -0.05"/>
    <parent link="link_chassis_0"/>
    <child link="link_caster_0"/>
  </joint>
        
  <link name="link_caster_0">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.04"/>
      </geometry>
      <material name="grey"/>
    </visual>
    <collision>
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.04"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="${EPSILON}" ixy="0" ixz="0" iyy="${EPSILON}" iyz="0" izz="${EPSILON}"/>
    </inertial>
  </link>

  <joint name="joint_chassis0_lwheel0" type="continuous">
    <origin rpy="0 0 0" xyz="0.12 0.16 0.01"/>
    <parent link="link_chassis_0"/>
    <child link="link_left_wheel_0" />
    <axis rpy="0 0 0" xyz="1 0 0"/>
  </joint>

  <link name="link_left_wheel_0">
    <visual>
      <origin rpy="0 ${PI/2} 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.04" radius="0.1"/>
      </geometry>
      <material name="orange"/>
    </visual>
    <collision>
      <origin rpy="0 ${PI/2} 0" xyz="0 0 0" />
      <geometry>
        <cylinder length="0.04" radius="0.1"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <origin rpy="0 ${PI/2} 0" xyz="0 0 0"/>
      <inertia ixx="${EPSILON}" ixy="0" ixz="0" iyy="${EPSILON}" iyz="0" izz="${EPSILON}"/>
    </inertial>
  </link>
          
  <joint name="joint_chassis0_rwheel0" type="continuous">
    <origin rpy="0 0 0" xyz="-0.12 0.16 0.01"/>
    <child link="link_right_wheel_0" />
    <parent link="link_chassis_0"/>
    <axis rpy="0 0 0" xyz="1 0 0"/>
  </joint>
    
  <link name="link_right_wheel_0">
    <collision>
      <origin rpy="0 ${PI/2} 0" xyz="0 0 0" />
      <geometry>
        <cylinder length="0.04" radius="0.1"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0 ${PI/2} 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.04" radius="0.1"/>
      </geometry>
      <material name="orange"/>
    </visual>
    <inertial>
      <mass value="0.5"/>
      <origin rpy="0 ${PI/2} 0" xyz="0 0 0"/>
      <inertia ixx="${EPSILON}" ixy="0" ixz="0" iyy="${EPSILON}" iyz="0" izz="${EPSILON}"/>
    </inertial>
  </link>
  
  <joint name="joint_log_hat1" type="revolute">
    <origin rpy="0 0 0" xyz="1 0 0"/>
    <parent link="link_log"/>
    <child link="link_hat_1"/>
    <axis rpy="0 0 0" xyz="0 0 1"/>
    <limit effort="0" lower="0" upper="0" velocity="0"/>
  </joint>

  <link name="link_hat_1">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 -0.04"/>
      <geometry>
        <cylinder length="0.08" radius="0.02"/>
      </geometry>
      <material name="orange"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 -0.04"/>
      <geometry>
        <cylinder length="0.08" radius="0.02"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <origin rpy="0 0 0" xyz="0 0 -0.04"/>
      <inertia ixx="${EPSILON}" ixy="0" ixz="0" iyy="${EPSILON}" iyz="0" izz="${EPSILON}"/>
    </inertial>
  </link>

  <joint name="joint_hat1_chassis1" type="continuous">
    <origin rpy="0 0 0" xyz="0.0 0 -0.13"/>
    <parent link="link_hat_1"/>
    <child link="link_chassis_1" />
    <axis rpy="0 0 0" xyz="0 0 1"/>
  </joint>

  <link name="link_chassis_1">
    <visual>
      <origin rpy="0 0 ${-PI/2}" xyz="0 0 0"/>
      <geometry>
        <box size="0.4 0.2 0.1"/>
      </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin rpy="0 0 ${-PI/2}" xyz="0 0 0"/>
      <geometry>
        <box size="0.4 0.2 0.1"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="2"/>
      <origin rpy="0 0 ${-PI/2}" xyz="0 0 0"/>
      <inertia ixx="${EPSILON}" ixy="0" ixz="0" iyy="${EPSILON}" iyz="0" izz="${EPSILON}"/>
    </inertial>
  </link>

  <joint name="joint_chassis1_caster1" type="fixed">
    <origin rpy="0 0 0" xyz="0 -0.15 -0.05"/>
    <parent link="link_chassis_1"/>
    <child link="link_caster_1"/>
  </joint>
        
  <link name="link_caster_1">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.04"/>
      </geometry>
      <material name="grey"/>
    </visual>
    <collision>
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.04"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="${EPSILON}" ixy="0" ixz="0" iyy="${EPSILON}" iyz="0" izz="${EPSILON}"/>
    </inertial>
